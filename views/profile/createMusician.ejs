<body>
  <nav>
    <a href="/profile">Your Profile</a>
    <a href="/profile/musicianList">Musician List</a>
    <a href="/profile/createBand">Create Band</a>
    <a href="/profile/bandList">Band List</a>
  </nav>
  <h1>Create Musician</h1>
  <form action="/profile/createMusician" method="post">
    <label for="name">Name:</label>
    <input
      type="text"
      id="name"
      name="name"
      style="margin-bottom: 10px"
    /><br />

    <label for="role">Role:</label>
    <select id="role" name="role" style="margin-bottom: 10px">
      <option value="Vocals">Singer</option>
      <option value="Guitar">Guitarist</option>
      <option value="Bass">Bassist</option>
      <option value="Drums">Drummer</option></select
    ><br />

    <label for="yearsOfExperience">Years of Experience:</label>
    <input
      type="number"
      id="yearsOfExperience"
      name="yearsOfExperience"
      style="margin-bottom: 10px"
    /><br />

    <label for="favouriteGenre">Favorite Genre:</label>
    <input
      type="text"
      id="genre"
      name="favouriteGenre"
      style="margin-bottom: 10px"
    /><br />

    <label for="favouriteBand">Favorite Band:</label>
    <input
      type="text"
      id="band"
      name="favouriteBand"
      style="margin-bottom: 10px"
    /><br />

    <button type="submit">Save Profile</button>
  </form>
</body>
