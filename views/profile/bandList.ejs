<div class="grid gap-3">
<h1>Band List</h1>
<% allBands.forEach((band) => { %>
<h5>Name: <%- band.name %></h5>

<h5>Members:</h5>
<% band.members.forEach((element) => { %>
<p><%- element.name %> : <%- element.role %></p>

<% }) %>

<p>Genre: <%- band.genre %></p>
<p>Date Founded: <%- band.dateFounded %></p>
<a href="/profile/delete/band/<%- band._id %>"><button class="btn btn-outline-light">Delete</button></a>
<a href="/profile/editBand/<%- band._id %>"><button class="btn btn-outline-light">Edit</button></a>
<% }) %>
</div>